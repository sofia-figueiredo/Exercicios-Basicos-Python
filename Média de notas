notas = []

nota1 = float(input("\n Digite o valor da primeira nota: "))
nota2 = float(input("\n Digite o valor da segunda nota: "))
nota3 = float(input("\n Digite o valor da terceira nota: "))
nota4 = float(input("\n Digite o valor da quarta nota: "))
nota5 = float(input("\n Digite o valor da quinta nota: "))

media = (nota1 + nota2 + nota3 + nota4 + nota5)/5

notas.append(f"As notas são: {nota1}, {nota2}, {nota3}, {nota4}, {nota5}, Média: {media}")
print("\n", notas)
